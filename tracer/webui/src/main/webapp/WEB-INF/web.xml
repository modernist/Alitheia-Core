<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  version="2.5">
  <display-name>TRACER-webui</display-name>
  <servlet>
    <servlet-name>ProxyServlet</servlet-name>
    <servlet-class>gr.tracer.webui.ProxyServlet</servlet-class>
    <init-param>
      <param-name>forwardTo</param-name>
      <param-value>http://localhost:8080/tracerapi</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProxyServlet</servlet-name>
    <url-pattern>/tracerproxy</url-pattern>
    <url-pattern>/tracerproxy/*</url-pattern>
  </servlet-mapping>
</web-app>