<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="eu.sqooss.service.db.GroupPrivilege" table="GROUP_PRIVILEGE">
    <composite-id>
      <key-many-to-one name="url" column="SERVICE_URL_ID" class="eu.sqooss.service.db.ServiceUrl"/>
      <key-many-to-one name="group" column="GROUP_ID" class="eu.sqooss.service.db.Group"/>
      <key-many-to-one name="pv" column="PRIVILEGE_VALUE_ID" class="eu.sqooss.service.db.PrivilegeValue"/>
    </composite-id>
  </class>  
</hibernate-mapping>

